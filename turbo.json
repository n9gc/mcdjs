{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"dev": {
			"persistent": true
		},
		"test": {
			"dependsOn": [
				"^test"
			],
			"inputs": [
				"./**/*.ts"
			]
		},
		"f-clear": {
			"inputs": [
				"./**/*.js",
				"./**/*.js.map",
				"./**/*.d.ts"
			],
			"outputs": [
				"./**/*.js",
				"./**/*.js.map",
				"./**/*.d.ts"
			],
			"cache": false
		},
		"clear": {
			"dependsOn": [
				"^f-clear"
			],
			"inputs": [
				"./**/*.js",
				"./**/*.js.map",
				"./**/*.d.ts"
			],
			"outputs": [
				"./**/*.js",
				"./**/*.js.map",
				"./**/*.d.ts"
			],
			"cache": false
		},
		"build": {
			"inputs": [
				"./**/*.ts"
			],
			"outputs": [
				"./**/*.js",
				"./**/*.js.map",
				"./**/*.d.ts"
			]
		}
	}
}