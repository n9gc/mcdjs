{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"dev": {
			"persistent": true
		},
		"devb": {
			"dependsOn": [
				"^devb"
			],
			"outputs": [
				"dist/**"
			],
			"inputs": [
				"dist/**/*.js",
				"package.json",
				"tsconfig.json",
				"lib/**/*.ts",
				"node_modules/**"
			]
		},
		"test-all": {
			"dependsOn": [
				"devb",
				"^test-all"
			],
			"inputs": [
				"dist/**",
				"test/**/*.js",
				"node_modules/**"
			]
		},
		"prod-test-all": {
			"dependsOn": [
				"build",
				"^prod-test-all"
			],
			"inputs": [
				"dist/**",
				"test/**/*.js",
				"node_modules/**"
			]
		},
		"build": {
			"dependsOn": [
				"devb"
			],
			"outputs": [
				"dist/**"
			],
			"inputs": [
				"dist/**/*.js",
				"package.json",
				"tsconfig.json",
				"lib/**/*.ts",
				"node_modules/**"
			]
		}
	}
}