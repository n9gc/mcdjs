		"@types/node": "",
		"aocudeo": "workspace:^",
		"reflect-metadata": "^0.1.13"
	},
	"devDependencies": {
		"devok": "workspace:^",
		"lethal-build": "^1.3.2",
		"promise-snake": "^1.2.2",
		"ts-loader": "^9.4.4",
