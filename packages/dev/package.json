{
	"name": "devok",
	"version": "1.0.0",
	"description": "全 TS 开发辅助工具",
	"main": "./index",
	"module": "./index",
	"types": "./index",
	"scripts": {
		"f-clear": "pnpm ts-node actor/clear -R",
		"build": "pnpm tsc",
		"pretest": "pnpm run build",
		"test": "pnpm ts-node tester/import -R=\"[['index.js',{webpack:{cmd:'--target node'}}]]\"",
		"posttest": "pnpm run f-clear",
		"prepublishOnly": "pnpm run build",
		"postpublish": "pnpm run f-clear"
	},
	"dependencies": {
		"lethal-build": "^1.3.2",
		"export-tester": "^1.0.13",
		"@types/node": "^18.16.3",
		"promise-snake": "^1.3.3"
	},
	"devDependencies": {
		"webpack-cli": ""
	},
	"files": [
		"*",
		"!test",
		"!.turbo",
		"!coverage",
		"!**/*.ts",
		"!*.swp",
		"!.nyc_output",
		"**/*.d.ts",
		"!tsconfig.json"
	],
	"keywords": [
		"development",
		"test",
		"build",
		"util",
		"typescript",
		"mcdjs"
	],
