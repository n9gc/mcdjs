{
	"compilerOptions": {
		"module": "CommonJS",
		"moduleResolution": "Node",
		"target": "ESNext",
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"strict": true,
		"strictBindCallApply": true,
		"strictFunctionTypes": true,
		"strictNullChecks": true,
		"strictPropertyInitialization": true,
		"alwaysStrict": true,
		"noImplicitThis": true,
		"noEmitOnError": true,
		"declaration": true,
		"sourceMap": true
	}
}